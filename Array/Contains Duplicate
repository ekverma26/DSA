import java.util.*;

class HelloWorld {
    
    public static boolean duplicate(int[] arr)
    {
        Set<Integer> set = new HashSet<>();
        
        for(Integer x: arr)
        {
            if(set.contains(x))
                return true;
            set.add(x);
            
        }
        return false;
    }

    
    
    public static void main(String[] args) {
        
        int arr[] = {1,2,3,4};
        
        
        System.out.println(duplicate(arr));
        
        
    }
}
